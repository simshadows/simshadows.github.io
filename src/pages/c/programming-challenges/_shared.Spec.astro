---
// TODO: Prefer to not use "initial". Make a proper css variable?

interface Props {
    href: string;
    cdate?: string;
}

const {
    href,
    cdate,
} = Astro.props;

if (!href) throw new Error("Blank href.");
---

<span style="font-size: initial;">
    [<a target="_blank" href={href}>Spec</a>]{cdate && <> ({cdate})</>}
</span>


---
import Layout from "@layouts/BaseLayout.astro";
import {fm} from "@helpers/frontmatter";
import {getPageTree} from "@helpers/experimental-glob";

import ExperimentalPageTree from "@components/ExperimentalPageTree";

export const frontmatter = fm({
    title: "Experimental Glob",
    description: "An experimental page for globbing pages.",
    keywords: ["experimental"],
});
---

<Layout {...frontmatter}>
    <ExperimentalPageTree tree={getPageTree()}/>
</Layout>
